{
  "expo": {
    "name": "GoodRunss",
    "slug": "goodrunss-mobile",
    "owner": "anthonye4258",
    "version": "1.0.0",
    "scheme": "goodrunss",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0A0A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.goodrunss.app",
      "buildNumber": "110",
      "deploymentTarget": "15.1",
      "associatedDomains": [
        "applinks:goodrunss.com",
        "applinks:studios.goodrunss.com"
      ],
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "GoodRunss needs your location to show nearby trainers, courts, and studios.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "GoodRunss needs your location to show nearby trainers, courts, and studios.",
        "NSCameraUsageDescription": "GoodRunss needs camera access to upload profile pictures and venue photos.",
        "NSPhotoLibraryUsageDescription": "GoodRunss needs photo library access to upload pictures.",
        "NSMicrophoneUsageDescription": "GoodRunss needs microphone access for voice messages.",
        "NSHealthShareUsageDescription": "GoodRunss needs access to read your workout data to provide personalized recommendations.",
        "NSHealthUpdateUsageDescription": "GoodRunss needs access to save your workout sessions to Apple Health.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#84CC16"
      },
      "package": "com.goodrunss.mobile",
      "versionCode": 2,
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "goodrunss"
            },
            {
              "scheme": "https",
              "host": "goodrunss.com",
              "pathPrefix": "/"
            },
            {
              "scheme": "https",
              "host": "studios.goodrunss.com",
              "pathPrefix": "/"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow GoodRunss to use your location to show nearby trainers, courts, and studios."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "GoodRunss needs access to your photos to upload profile pictures and venue photos.",
          "cameraPermission": "GoodRunss needs camera access to take photos."
        }
      ],
      "expo-notifications",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1"
          },
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "minSdkVersion": 23
          }
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "54c9b3c6-9f21-44cc-921b-0fa2db35b4a4"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/54c9b3c6-9f21-44cc-921b-0fa2db35b4a4"
    }
  }
}